{
  "name": "grassroots-archive",
  "version": "0.0.1",
  "description": "A sample Node.js app using Express 4",
  "engines": {
    "node": "6.10.2"
  },
  "main": "server.js",
  "scripts": {
    "build": "webpack -p --config config/webpack.config.common.js --progress",
    "dev": "webpack-dev-server --config config/webpack.config.common.js --inline --progress --port 8080",
    "start": "node --optimize_for_size --max_old_space_size=460 --gc_interval=100 server/server.js",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "@angular/common": "4.1.3",
    "@angular/compiler": "4.1.3",
    "@angular/compiler-cli": "4.2.2",
    "@angular/core": "4.1.3",
    "@angular/forms": "4.2.2",
    "@angular/http": "4.2.2",
    "@angular/platform-browser": "4.1.3",
    "@angular/platform-browser-dynamic": "4.1.3",
    "@angular/router": "4.2.2",
    "core-js": "2.4.1",
    "express": "4.15.3",
    "generic-pool": "3.1.7",
    "mysql": "2.13.0",
    "rxjs": "5.4.0",
    "zone.js": "0.8.12"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "license": "MIT",
  "author": "DongKyoo Kim",
  "devDependencies": {
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "3.1.3",
    "html-webpack-plugin": "2.28.0",
    "rimraf": "2.6.1",
    "typescript": "2.3.4",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.4.5"
  }
}
