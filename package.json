{
  "name": "grassroots-archive",
  "version": "0.0.1",
  "description": "A sample Node.js app using Express 4",
  "engines": {
    "node": "8.1.4"
  },
  "main": "server.js",
  "scripts": {
    "build": "rimraf public && webpack --config config/webpack.config.prod.js --progress",
    "demo": "npm run build && node -r dotenv/config server/server.js --optimize_for_size --max_old_space_size=460 --gc_interval=100",
    "nodemon": "nodemon --exec \"node -r dotenv/config server/server.js --trace-sync-io\"",
    "dev": "concurrently \"npm run nodemon\" \"webpack-dev-server --config config/webpack.config.dev.js\"",
    "start": "pm2 start --attach server/server.js --node-args \"--optimize_for_size --max_old_space_size=460 --gc_interval=100\" && pm2 logs all",
    "heroku-postbuild": "npm run build && pm2 install pm2-logrotate"
  },
  "dependencies": {
    "@angular/common": "4.3.0",
    "@angular/compiler": "4.3.0",
    "@angular/compiler-cli": "4.3.0",
    "@angular/core": "4.3.0",
    "@angular/forms": "4.3.0",
    "@angular/http": "4.3.0",
    "@angular/platform-browser": "4.3.0",
    "@angular/platform-browser-dynamic": "4.3.0",
    "@angular/router": "4.3.0",
    "@types/node": "8.0.17",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "3.2.1",
    "body-parser": "1.17.2",
    "compression-webpack-plugin": "1.0.0",
    "core-js": "2.4.1",
    "css-loader": "0.28.4",
    "debug": "2.6.8",
    "express": "4.15.3",
    "express-static-gzip": "0.3.0",
    "extract-text-webpack-plugin": "3.0.0",
    "html-loader": "0.5.0",
    "html-webpack-plugin": "2.30.1",
    "jsonwebtoken": "7.4.1",
    "morgan": "1.8.2",
    "passport": "0.3.2",
    "passport-facebook": "2.1.1",
    "pm2": "2.6.1",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.1",
    "rxjs": "5.4.2",
    "source-list-map": "2.0.0",
    "style-loader": "0.18.2",
    "typescript": "2.4.1",
    "webpack": "3.4.1",
    "webpack-merge": "4.1.0",
    "webpack-sources": "1.0.1",
    "xml2js": "0.4.17",
    "zone.js": "0.8.16"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "license": "MIT",
  "author": "DongKyoo Kim",
  "devDependencies": {
    "concurrently": "3.5.0",
    "dotenv-safe": "4.0.4",
    "nodemon": "1.11.0",
    "webpack-dev-server": "2.5.1"
  }
}
