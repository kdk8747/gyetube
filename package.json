{
  "name": "grassroots-archive",
  "version": "0.0.1",
  "description": "A sample Node.js app using Express 4",
  "engines": {
    "node": "8.1.4"
  },
  "main": "server.js",
  "scripts": {
    "build": "rimraf public && webpack --config config/webpack.config.prod.js --progress",
    "demo": "npm run build && node -r dotenv/config server/server.js --optimize_for_size --max_old_space_size=460 --gc_interval=100",
    "nodemon": "nodemon --exec \"node -r dotenv/config server/server.js --trace-sync-io\"",
    "dev": "concurrently \"npm run nodemon\" \"webpack-dev-server --config config/webpack.config.dev.js\"",
    "start": "pm2 start --attach server/server.js --node-args \"--optimize_for_size --max_old_space_size=460 --gc_interval=100\" && pm2 logs all",
    "heroku-postbuild": "npm run build && pm2 install pm2-logrotate"
  },
  "dependencies": {
    "@angular/common": "4.1.3",
    "@angular/compiler": "4.1.3",
    "@angular/compiler-cli": "4.2.2",
    "@angular/core": "4.1.3",
    "@angular/forms": "4.2.2",
    "@angular/http": "4.2.2",
    "@angular/platform-browser": "4.1.3",
    "@angular/platform-browser-dynamic": "4.1.3",
    "@angular/router": "4.2.2",
    "aws-sdk": "2.82.0",
    "body-parser": "1.17.2",
    "core-js": "2.4.1",
    "debug": "2.6.8",
    "express": "4.15.3",
    "express-static-gzip": "0.2.2",
    "generic-pool": "3.1.7",
    "mysql": "2.13.0",
    "rxjs": "5.4.0",
    "zone.js": "0.8.12"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "license": "MIT",
  "author": "DongKyoo Kim",
  "devDependencies": {
    "@types/node": "8.0.0",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "3.1.3",
    "compression-webpack-plugin": "0.4.0",
    "concurrently": "3.4.0",
    "css-loader": "0.28.4",
    "dotenv-safe": "4.0.4",
    "extract-text-webpack-plugin": "2.1.2",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "nodemon": "1.11.0",
    "pm2": "2.5.0",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.1",
    "style-loader": "0.18.2",
    "typescript": "2.3.4",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.4.5",
    "webpack-merge": "4.1.0"
  }
}
